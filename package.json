{
  "homepage": "/main",
  "name": "main",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^2.4.2",
    "@chakra-ui/theme": "^2.1.13",
    "@chakra-ui/theme-tools": "^2.0.12",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@radix-ui/react-icons": "^1.1.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.4",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "axios": "^0.27.2",
    "framer-motion": "^6.5.1",
    "i18next": "^21.9.0",
    "i18next-browser-languagedetector": "^6.1.5",
    "mathjax-full": "^3.2.2",
    "mathjax-react": "^2.0.1",
    "react": "^18.2.0",
    "react-cookie-consent": "^8.0.1",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^2.1.0",
    "react-i18next": "^11.18.3",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "ESLINT_NO_DEV_ERRORS='true'  react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN='true' react-scripts build",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.tsx,.ts src/",
    "fix": "eslint --fix --ignore-path .gitignore --ext .js,.jsx,.tsx,.ts src/",
    "prepare": "cd .. && husky install main/.husky",
    "test": "react-scripts test --testURL=http://localhost:3000/main?abbrevName=automation_10",
    "eject": "react-scripts eject",
    "prestart": "rm -rf node_modules/.cache/"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "react-app",
      "react-app/jest",
      "airbnb-typescript",
      "prettier"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "ignorePatterns": [
      "src/i18n.js"
    ],
    "plugins": [
      "react",
      "prettier"
    ],
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "all",
          "jsxBracketSameLine": true,
          "printWidth": 80,
          "tabWidth": 2,
          "useTabs": true,
          "semi": true
        }
      ],
      "no-unused-vars": "error",
      "no-console": "warn",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx",
            ".tsx"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.25.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0"
  },
  "proxy": "http://mybob.amplifire.me:8080"
}
