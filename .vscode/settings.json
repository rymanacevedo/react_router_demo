{
    "editor.formatOnSave": true,
    "eslint.codeActionsOnSave.rules": {
        "singleQuote": true,
        "trailingComma": "all",
        "jsxBracketSameLine": true,
        "printWidth": 80,
        "tabWidth": 2,
        "useTabs": true,
        "semi": true
    },
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "typescript.tsdk": "node_modules/typescript/lib",
}